#version 430

struct particle {
	mediump vec4	currPos;
	mediump vec4	speedVec;
	mediump float ttl;
};

layout(location=0) in particle part;


uniform mat4 ViewMatrix;
uniform mat4 ProjectionMatrix;

void main(void)
{
  vec4 asd = vec4(part.currPos.x, part.currPos.y - 2.0, part.currPos.za);
  gl_Position =asd;
}
